{
	"type": "edit-entry",
	"database": "DataTables::getDatabase()",
	"table": "tables",
	"actions": {
		"save-close": {
			"page": "Tables",
			"message": "Table Renamed",
			"text": "Rename"
		},
		"discard": {
			"page": "Tables"
		},
		"delete": {
			"page": "Tables",
			"question": "Delete Table `{{name}}`?\nThis CANNOT be undone!",
			"message": "Table `{{name}}` and Associated Data Deleted",
			"callback": "DataTables::removeTable"
		}
	},
	"fields": {
		"name": {
			"maxlen": 40
		}
	}
}
