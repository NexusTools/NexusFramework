{
	"name": "Logger",
	"database": {
		"logs": {
			"user": "INTEGER",
			"level": "TINYINT",
			"section": {
				"type": "VARCHAR(200)",
				"case-insensative": 1
			},
			"subsection": {
				"type": "VARCHAR(200)",
				"case-insensative": 1,
				"allow-null": 1
			},
			"description": "VARCHAR(200)",
			"address": "VARCHAR(32)",
			"details": {
				"type": "BLOB",
				"allow-null": 1
			},
			"stack": {
				"type": "BLOB",
				"allow-null": 1
			}
		}
	}
}
